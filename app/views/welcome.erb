<div class="container">
  <div>
    <% if logged_in? %>
      <br>
      <br>
      <h2>Welcome, <%=current_user.username %></h2>
    <% else %>
      <h2>Welcome</h2>
      <h3>Please Log In or Sign Up below:</h3>
      <br>
      <a href="/signup" ><button type="button" >Sign Up</button></a>
      <br>
      <br>
      <a href="/login"><button type="button" >Log In</button></a>
    <%end %>
  </div>
</div>
